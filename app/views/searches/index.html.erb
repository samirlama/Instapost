<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
	<div class="container">
		<table class="table table-striped">
			<% if !@users.nil? && !@users.empty? %>
			<% @users.each do |user| %>

    <thead>
    	
      <tr>
        <th><%=image_tag user.photo , :style=>"width:50px; height:50px;"%> </th>
        <th><%=link_to user.name , user_path(user) %></th>
      </tr>
      
    </thead>
    <% end %>
    <% else %>
    	No results found 
    <% end %>	
</table>
</div>
</body>
</html>